    errorPanelHeight: 542
    lintDebug: true
    showErrorTabLine: true
    showInfoMessages: true
    statusBar: "Show all errors"
  "linter-erb":
    erbExecutablePath: "/usr/local/opt/rbenv/shims/erb"
    trimMode: "-"
  metrics:
    userId: "44edc941840c87dd1cf98a0d605a1fa48c3e1883"
  "python-autopep8": {}
  "release-notes":
    viewedVersion: "0.94.0"
  "rst-preview-pandoc":
    pandocOpts: "-frst -thtml --webtex"
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  welcome:
    showOnStartup: false
