"*":
  core:
    disabledPackages: [
      "git-plus"
      "rst-preview"
      "linter-rubocop"
      "ginkgo-and-gomega-snippets"
      "activate-power-mode"
      "linter-erb"
      "linter-shellcheck"
    ]
    projectHome: "/Users/ali/code"
    themes: [
      "dracula-ui"
      "dracula-theme"
    ]
  editor:
    fontFamily: "Courier New"
    invisibles: {}
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "d4efaa67-4ea3-de6f-ea14-09270d232311"
